{
  "version": 2,
  "builds": [
    {
      "src": "app/api//*.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": ".next/**, *.cache/**, node_modules/**, public/**, .git/**, interactions.json, venv/**, faq_data.json, *.md"
      }
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ]
}
